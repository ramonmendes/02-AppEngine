<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>ListarTelefone</servlet-name>
		<servlet-class>br.com.cit.lab.servlet.ListarTelefone</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IncluirTelefone</servlet-name>
		<servlet-class>br.com.cit.lab.servlet.IncluirTelefone</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogAppsServlet</servlet-name>
		<servlet-class>br.com.cit.lab.servlet.LogAppsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IncluirTelefone</servlet-name>
		<url-pattern>/app/telefone/incluir</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListarTelefone</servlet-name>
		<url-pattern>/app/telefone/listar</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogAppsServlet</servlet-name>
		<url-pattern>/queue/log</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<security-constraint>
		<web-resource-collection>
			<url-pattern>/app/telefone/*</url-pattern>
			<url-pattern>/pages/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
	</security-constraint>
</web-app>
